{
  "name": "react_redux_cart",
  "version": "1.0.0",
  "description": "Cart demo application with React and redux",
  "main": "index.js",
  "scripts": {
    "dev": "webpack -d --watch",
    "dev-server": "webpack-dev-server --inline --hot --watch --host 0.0.0.0 --port 8044",
    "start": "npm run dev-server",
    "lint": "eslint src",
    "prebuild": "npm run test",
    "build": "webpack -p && cd static && tar cvf ../dist/dist.tar.gz *",
    "pretest": "npm run lint",
    "test": "mocha",
    "pretestJunit": "npm run lint",
    "testJunit": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=test/test-results.xml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.2.0",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
